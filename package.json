  "license": "ISC",
  "devDependencies": {
    "concurrently": "^7.2.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.16"
  },
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-validator": "^6.14.0",
    "history": "^5.3.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.3.3",
    "shortid": "^2.2.16"
  }
}
